PRNLEV 2

SET PATH ../common

READ RTF CARD        NAME -
 @PATH/zeroed.top



READ PARAM CARD        NAME -
 @PATH/params_mtp.par

OPEN UNIT 10 READ CARD NAME @PATH/phbr.dyna.solute.pdb
READ SEQUENCE PDB UNIT 10
GENERATE SOLU
REWIND UNIT 10
READ COOR PDB UNIT 10
CLOSE UNIT 10








BOMLEV 0

NBONDS NBXMOD 5 ATOM CDIEL EPS 1.0 SHIFT VATOM VDISTANCE -
  VSWITCH CUTNB 99.0 CTOFNB 98.0 CTONNB 97. E14FAC 1.0
PERT SELE SEGI SOLU END
SCALAR RSCA SET 0. SELE SEGI SOLU END
SCALAR FBETA SET 5. SELE ALL END




















DYNA LEAP STRT NSTEP SSS TIMESTEP 0.001 -
  NTRFRQ 100 -
  IPRFRQ 0 INBFRQ -1 IMGFRQ 250 -
  LSTART 0.000000  LAMBDA 0.012500  LSTOP 0.025000  PSTART  80000 -
  PSTOP  200000  PSLOW LINCR 0.025000 PSSP - 
  TBATH 298. RBUF 0. ILBFRQ 10 FIRSTT 240. -
  -
  NPRINT 1000 NSAVC -1 -




STOP
